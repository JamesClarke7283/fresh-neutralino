{
  "applicationId": "com.example.neutralino-deno-fresh",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "src/neutralino",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "Neutralino Deno Fresh",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "neutralino-deno-fresh",
    "resourcesPath": "src/neutralino",
    "extensionsPath": "extensions",
    "clientLibrary": "/js/neutralino.js",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0"
  }
}